{
  "name": "@jill64/sveltekit-adapter-aws",
  "description": "ğŸ”Œ SveleteKit AWS adapter with multiple architecture",
  "version": "1.0.15",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/sveltekit-adapter-aws/issues",
  "homepage": "https://github.com/jill64/sveltekit-adapter-aws#readme",
  "files": [
    "dist",
    "embed",
    "cdk",
    "!*.test.*",
    "!*.spec.*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "adapter",
    "aws",
    "sveltekit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/sveltekit-adapter-aws.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "npm run build:adapter && cd packages/site && npm run dev",
    "build:all": "npm run build:adapter && npm run build:site && npx publint",
    "build:adapter": "npm run transpile:embed && npm run build:cf2 && npm run build:adapterOnly",
    "build:adapterOnly": "cd packages/adapter && npm run build",
    "build:cf2": "npx cf2-builder cf2/src/index.ts cdk/external/cf2.js",
    "transpile:embed": "cd embed && npm run transpile",
    "build:site": "cd packages/site && npm run build",
    "preview": "npm run build:adapter && cd packages/site && npm run preview",
    "prepack": "npm run build:adapter",
    "format": "npm run format:lib && npm run format:site",
    "format:lib": "npx prettier --write .",
    "format:site": "cd packages/site && npm run format",
    "lint": "npm run lint:lib && npm run lint:embed && npm run lint:site",
    "lint:lib": "npx eslint . && tsc",
    "lint:embed": "cd embed && npm run lint",
    "lint:site": "npm run build:adapter && cd packages/site && npm run lint"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0"
  },
  "devDependencies": {
    "@playwright/test": "1.40.0",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "eslint-plugin-deprecation": "2.0.0",
    "cf2-builder": "1.0.3"
  },
  "dependencies": {
    "@jill64/types-lambda": "1.0.10",
    "@sveltejs/kit": "1.27.6",
    "@types/mime-types": "2.1.4",
    "@types/node": "20.10.0",
    "aws-cdk": "2.110.1",
    "aws-cdk-lib": "2.110.1",
    "constructs": "10.3.0",
    "dotenv": "16.3.1",
    "esbuild": "0.19.8",
    "mime-types": "2.1.35",
    "nanoid": "5.0.3",
    "typescript": "5.3.2"
  }
}